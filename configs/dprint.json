{
  "incremental": true,
  "useTabs": false,
  "indentWidth": 2,
  "lineWidth": 100,
  "typescript": {},
  "json": {
    "associations": [
      "**/*.schema"
    ]
  },
  "markdown": {},
  "toml": {},
  "includes": [
    "**/*.{ts,tsx,js,jsx,cjs,mjs,json,md,toml,schema}"
  ],
  "excludes": [
    "**/node_modules",
    "**/dist",
    "**/*-lock.json"
  ],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.62.0.wasm",
    "https://plugins.dprint.dev/json-0.14.0.wasm",
    "https://plugins.dprint.dev/markdown-0.12.1.wasm",
    "https://plugins.dprint.dev/toml-0.5.3.wasm"
  ]
}
